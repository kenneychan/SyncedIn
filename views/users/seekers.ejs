<%- include('../partials/header') %>

<blockquote class="blockquote text-center">
  <p class="mb-0">You're gonna like the person you hire, I guarantee it.</p>
  <br>
  <footer class="blockquote-footer">The Men's Warehouse guy, probably.</cite></footer>
</blockquote>
<table class="table table-hover rounded overflow-hidden custom-table" class="border " >
   <caption>List of Job Seekers</caption>
  <tbody>
    <% users.forEach (function (u) { %>
    <tr>
      <td class="review-user">
        <img
          alt="avatar"
          src="<%= u.avatar %>"
          referrerpolicy="no-referrer"
        />&nbsp; &nbsp;<%= u.name %>
      </td>
      <td><a href="/users/<%= u._id %>" class="btn btn-info">Profile</a></td>
    </tr>
    <% }) %>
  </tbody>
</table>

<%- include('../partials/footer') %>